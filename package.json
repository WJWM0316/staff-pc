{
  "name": "staff-web-frontend",
  "version": "1.0.0",
  "private": true,
  "ahthor": "lizifa",
  "scripts": {
    "serve": "vue-cli-service serve --mode local",
    "build:dev": "vue-cli-service build --mode development",
    "build:pro": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cropperjs": "^1.4.1",
    "element-ui": "^2.4.5",
    "js-cookie": "^2.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "moment": "^2.22.2",
    "qs": "^6.5.2",
    "sass-loader": "^7.1.0",
    "scss-loader": "^0.0.1",
    "swiper": "^4.4.2",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-router": "^3.0.1",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1",
    "web-storage-cache": "^1.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.7",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.7",
    "@vue/cli-service": "^3.0.0-rc.7",
    "element-theme-chalk": "git+https://github.com/ElementUI/theme-chalk.git",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "node-sass": "^7.0.0",
    "sass": "^1.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-debugger": "off",
      "indent": [
        "error",
        2
      ],
      "space-before-function-paren": [
        0,
        "always"
      ],
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
